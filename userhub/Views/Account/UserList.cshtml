@model IList<UserRowViewModel>
<div class="container">
    <div class="row">

        <div class="col-md-12">
            <h4>User List</h4>

            <div class="table-reponsive">

                <table id="tblUsers" class="table table-bordered table-stripped">
                    <thead>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Company</th>
                        <th>Active</th>
                        <th>Edit</th>
                    </thead>
                    <tbody>
                        @foreach(var usrItem in Model)
                        {
                            @Html.Partial("_UserRow",usrItem);
                        }
                    </tbody>    
                        
                </table>                

            </div>                
        </div>    

    </div>    
</div>    